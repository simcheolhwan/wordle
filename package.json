    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.31.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-svgr": "^2.4.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint --max-warnings=0",
